<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

   <hibernate-mapping>
   <class name="zss.excise.entity.Orders" table="orders">
      <id name="ordersId" column="ordersId">
      	<generator class="native"></generator>
      </id>
      <property name="userName" column="userName" type="java.lang.String"/>
      <property name="goodsId" column="goodsId" type="java.lang.Integer"/>
      <property name="goodsAmount" column="goodsAmount" type="java.lang.Integer"/>
      <property name="ordersSum" column="ordersSum" type="java.lang.Double"/>
   </class>

</hibernate-mapping>