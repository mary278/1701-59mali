<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

   <hibernate-mapping>
   <class name="zss.excise.entity.User" table="user">
      <id name="userId" column="userId">
      	<generator class="native"></generator>
      </id>
      <property name="userName" column="userName" type="java.lang.String" unique="true"/>
      <property name="userPassword" column="userPassword" type="java.lang.String"/>
      <property name="userPhone" column="userPhone" type="java.lang.String"/>
   </class>

</hibernate-mapping>